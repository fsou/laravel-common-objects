{
  "name": "umbrellio/laravel-common-objects",
  "license": "MIT",
  "type": "library",
  "description": "Common objects for extending base Laravel functionality.",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Korben Dallas",
      "email": "pvsaintpe@umbrellio.biz"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
    "hemp/presenter": "^2.0"
  },
  "require-dev": {
    "umbrellio/code-style-php": "^1.0",
    "orchestra/testbench": "^3.5",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "scripts": {
    "lint": [
      "ecs check --config=ecs.yml . --fix"
    ]
  },
  "autoload": {
    "psr-4": {
      "Umbrellio\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Umbrellio\\Common\\tests\\": "tests/"
    }
  }
}
